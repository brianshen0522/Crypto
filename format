[msg, signature]

msg
ip
["$ID", "!HELLO!"]
["$ID", "?IP?", "$ID"]

transport
["$ID", "!CAST!", "$destination", "$msg"]
ex:["001", "!CAST!", "["all"]", "hi"]
   ["001", "!CAST!", "["002", "003", "004", "005"]", "hi"]

transaction
[$ACCOUNT, "!remit!", $PAYEE, $AMOUNT]
[[$ACCOUNT, "!remit!", $PAYEE, $AMOUNT], ["$ID", !verified!, $SIGNATURE]]